{
  "name": "@coinbase/cdp-api-client",
  "version": "0.0.67",
  "type": "module",
  "files": [
    "dist/**",
    "!dist/**/*.tsbuildinfo"
  ],
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "axios": "^1.11.0"
  },
  "devDependencies": {
    "@size-limit/preset-big-lib": "^11.2.0",
    "@size-limit/webpack": "^11.2.0",
    "@size-limit/webpack-why": "^11.2.0",
    "size-limit": "^11.2.0"
  },
  "size-limit": [
    {
      "name": "full-package",
      "path": "./dist/esm/index.js",
      "running": false,
      "import": "*",
      "limit": "40 KB"
    }
  ],
  "scripts": {
    "build": "pnpm run clean && pnpm run check:types && vite build --config ../../vite.config.base.ts",
    "build:watch": "vite build --config ../../vite.config.base.ts --watch",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf dist",
    "clean:all": "pnpm clean && rm -rf node_modules",
    "size-limit": "size-limit",
    "size-limit:why": "size-limit --why",
    "test": "vitest",
    "test:run": "vitest --run"
  }
}